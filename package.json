  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "file-loader": "^6.2.0",
    "foundation-sites": "^6.6.3",
    "html-webpack-plugin": "^4.5.0",
    "npm-watch": "^0.7.0",
    "postcss": "^8.1.4",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -Rf static/*",
    "build:prod": "webpack",
    "build:prod:nominify": "webpack --env nominify",
    "build:debug": "webpack --env debug",
    "prod": "npm run build:prod",
    "debug": "npm run build:debug",
    "clean:debug": "run-s clean debug",
    "clean:prod": "run-s clean prod",
    "build": "run-p build:prod build:debug",
    "clean:build": "run-s clean build",
    "build:nominify": "run-p build:prod:nominify build:debug",
    "deploy": "run-s clean build",
    "deploy:nominify": "run-s clean build:nominify",
    "watch:prod": "webpack --watch",
    "watch:debug": "webpack --env debug --watch",
    "watch": "run-p watch:prod watch:debug",
    "server:prod": "webpack-dev-server --watch",
    "server:debug": "webpack-dev-server --env debug --watch",
    "develop": "npm run server:debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hipikat/hpk.io.git"
  },
